{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "BoostGPT Documentation",
  "colors": {
    "primary": "#4473ff",
    "light": "#f5f3ed",
    "dark": "#4473ff"
  },
  "favicon": "/favicon.svg",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "icons": {
    "library": "lucide"
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://app.boostgpt.co"
      },
      {
        "label": "Support",
        "href": "https://discord.gg/KGhz5SnyXM"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://app.boostgpt.co/signup"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart",
              "authentication"
            ]
          },
          {
            "group": "Providers & Models",
            "pages": [
              "providers",
              "models" 
            ]
          },
          {
            "group": "SDK",
            "pages": [
              "sdk/overview",
              {
                "group": "Core SDK",
                "pages": [
                  "sdk/core/installation",
                  "sdk/core/api-reference"
                ]
              },
              {
                "group": "Router SDK",
                "pages": [
                  "sdk/router/installation",
                  "sdk/router/getting-started",
                  "sdk/router/api-reference",
                  "sdk/router/custom-adapters",
                  {
                    "group": "Adapters",
                    "pages": [
                      "sdk/router/adapters/discord",
                      "sdk/router/adapters/telegram",
                      "sdk/router/adapters/slack",
                      "sdk/router/adapters/whatsapp",
                      "sdk/router/adapters/crisp"
                    ]
                  }
                ]
              }
            ]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/deployment",
              "guides/troubleshooting"
            ]
          },
          {
            "group": "Examples",
            "pages": [
              "examples/use-cases"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/introduction"
            ]
          }
        ]
      },
       {
        "tab": "Changelog",
        "groups": [
          {
            "group": "Changelog",
            "pages": [
              "changelog"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/boostgpt",
          "icon": "github"
        },
        {
          "anchor": "Discord Community",
          "href": "https://discord.gg/KGhz5SnyXM",
          "icon": "discord"
        },
        {
          "anchor": "Dashboard",
          "href": "https://app.boostgpt.co",
          "icon": "gauge"
        }
      ]
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/boostgpthq",
      "github": "https://github.com/boostgpt",
      "discord": "https://discord.gg/KGhz5SnyXM"
    }
  },
  "api": {
    "baseUrl": "https://api.boostgpt.co",
    "auth": {
      "method": "bearer"
    }
  }
}