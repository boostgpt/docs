                    "items": {
                      "type": "string"
                    }
                  },
                  "top": {
                    "type": "integer"
                  },
                  "instruction": {
                    "type": "string"
                  },
                  "max_reply_tokens": {
                    "type": "integer"
                  },
                  "stream": {
                    "type": "boolean"
                  },
                  "chat_id": {
                    "type": "string"
                  }
                }
              }
            }
